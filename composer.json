{
    "name": "comodojo/cache",
    "description": "Very fast PHP data caching across multiple storage engines",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cache",
        "comodojo",
        "xattr",
        "redis",
        "memcached",
        "apc",
        "apcu",
        "xcache"
    ],
    "homepage": "https://comodojo.org",
     "support": {
        "issues": "https://github.com/comodojo/cache/issues"
    },
    "authors": [
        {
            "name": "Marco Giovinazzi",
            "email": "marco.giovinazzi@comodojo.org",
            "homepage": "http://jme.altervista.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Comodojo\\Cache\\": "src/Comodojo/Cache"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "comodojo/database": "1.*",
        "comodojo/exceptions": "@stable",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.0"
    },
    "suggest": {
        "ext-xattr": "Fastest cache files handling via extended attributes",
        "ext-redis": "Enable redis provider",
        "ext-memcached": "Enable Memcached provider",
        "ext-xcache": "Enable XCache provider",
        "ext-apc": "Enable Apc provider",
        "ext-apcu": "Enable Apcu provider"
    }
}
