{
    "name": "comodojo/cache",
    "description": "Extensible, multi layer object cache interface",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cache",
        "comodojo"
    ],
    "homepage": "http://comodojo.org",
     "support": {
        "issues": "https://github.com/comodojo/cache/issues"
    },
    "authors": [
        {
            "name": "Marco Giovinazzi",
            "email": "marco.giovinazzi@comodojo.org",
            "homepage": "http://jme.altervista.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Comodojo\\Exception\\": "src/Exception",
            "Comodojo\\Cache\\": "src"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "comodojo/database": "1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "scrutinizer/ocular": "~1.1"
    },
    "suggest": {
        "ext-xattr": "Fastest cache files handling via extended attributes"
    }
}